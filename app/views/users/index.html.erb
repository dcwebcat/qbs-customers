<div class="container">
<h1>All Customers</h1>
<hr/>

<% @users.each do |user| %>
    <div class="row well">
        <div class="col-md-2">
            <h4><%= link_to user.customer_number, user_path(user) %></h4>
        </div>
        <div class="col-md-10">
            <h4><%= user.company %></h4>
        </div>
    </div>
<% end %>

</div>