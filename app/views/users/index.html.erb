<div class="container">
<h1>All Customers</h1>
<hr/>
<div class="center">
<%= link_to "Add Customers", new_user_path, class: "center btn btn-success btn-white-text" %>
<br><br>
<%= will_paginate %>
</div>
<% @users.each do |user| %>
    <div class="col-md-8 col-md-offset-2 row well">
        <div class="col-md-2">
            <h4><%= link_to user.customer_number, user_path(user) %></h4>
        </div>
        <div class="col-md-10">
            <h4><%= user.company %></h4>
        </div>
    </div>
<% end %>
<div class="center">
<%= will_paginate %>
</div>
</div>